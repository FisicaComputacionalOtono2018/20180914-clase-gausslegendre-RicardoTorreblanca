        "\n",
        "def PolinomioLegendre(n,x):\n",
        "  if n==0:\n",
        "     return 0\n",
        "  elif n==1:\n",
        "     return x\n",
        "  elif n==2:\n",
        "    return 0.5*(3*x**2-1)\n",
        "  elif n==3:\n",
        "    return 0.5*(5*x**3*x)\n",
        "n= int(input(\"Dame un numero entero:\"))\n",
        "x= input (\"Dame un numero entre -1 y 1:\")\n",
        "print(PolinomioLegendre(n,x))"
      ],
